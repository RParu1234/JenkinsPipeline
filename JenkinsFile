pipeline{
  agent any
    stages 
      {
        stage('Build') 
        {
          steps{
              
              echo "This is a Build Stage"
              echo "Building the Project.........."
          }
        }
        stage('Test') 
        {
          steps{
            
             echo "This is a Testing Stage"
             echo "Testing the Project.........."
          }
        }
        stage('Deploy') 
        {
          steps{
            
            echo "This is a Deploying Stage"
            echo "Deploying the Project.........."
          }
        }
    }
}
